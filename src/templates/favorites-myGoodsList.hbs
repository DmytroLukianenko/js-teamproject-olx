<section class="favorites-myGoods container">
  <h1 class="favorites-myGoods__title">{{title}}</h1>
  <ul class="favorites-myGoods__list list">
    {{#each this}}
    <li class="favorites-myGoods__item" data-card="{{this._id}}">
      <div class="favorites-myGoods__item-img-wrapper">

        <img class="favorites-myGoods__item-img" src="{{#with this.imageUrls}}{{this.[0]}}{{/with}}"
          alt="{{this.title}}" />

      </div>

      <h2 class="favorites-myGoods__item-title">{{this.title}}</h2>
      <p class="favorites-myGoods__item-price">{{this.price}} <span> &#8364;</span></p>
    </li>
    {{/each}}
  </ul>

</section>